@model IEnumerable<Natural_Age.Models.Cita>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Especialista.Nombre_Especialista)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Especialista.Apellido_Especialista)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Paciente.Nombre_Paciente)
